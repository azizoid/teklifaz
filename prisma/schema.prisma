generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Repository {
  id          Int      @id @default(autoincrement())
  name        String   @unique
  repo_name   String 
  owner       String
  stars       Int
  activity    Int
  details     Json
  etag        String?
  lastUpdated DateTime @default(now())
  repoDetails RepoDetails?
}

model RepoDetails {
  id           String      @id
  contributors Json
  repository   Repository  @relation(fields: [id], references: [name])
}